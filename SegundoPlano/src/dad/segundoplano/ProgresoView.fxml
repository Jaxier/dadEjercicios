<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.control.ProgressIndicator?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>


<VBox alignment="CENTER" spacing="5.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/8.0.141">
   <children>
      <Label fx:id="labelMensaje" text="Label" />
      <ProgressIndicator fx:id="progresoIndicator" progress="0.0" />
      <ProgressBar fx:id="progresoBar" prefWidth="200.0" progress="0.0" />
      <HBox alignment="CENTER" spacing="5.0">
         <children>
            <Button fx:id="buttonIniciar" mnemonicParsing="false" onAction="#OnActionIniciar" text="Iniciar" />
            <Button fx:id="buttonDetener" mnemonicParsing="false" onAction="#OnActionDetener" text="Detener" />
         </children>
      </HBox>
   </children>
   <padding>
      <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
   </padding>
</VBox>
