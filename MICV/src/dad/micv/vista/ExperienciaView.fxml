<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<HBox maxWidth="1.7976931348623157E308" spacing="5.0" xmlns="http://javafx.com/javafx/8.0.141" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <TableView HBox.hgrow="ALWAYS">
        <columns>
          <TableColumn prefWidth="75.0" text="Desde" />
          <TableColumn prefWidth="75.0" text="Hasta" />
            <TableColumn prefWidth="100.0" text="Denominación" />
            <TableColumn prefWidth="75.0" text="Empleador" />
        </columns>
      </TableView>
      <VBox spacing="5.0">
         <children>
            <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Añadir" />
            <Button maxWidth="1.7976931348623157E308" mnemonicParsing="false" text="Eliminar" />
         </children>
      </VBox>
   </children>
   <padding>
      <Insets bottom="5.0" left="5.0" right="5.0" top="5.0" />
   </padding>
</HBox>
